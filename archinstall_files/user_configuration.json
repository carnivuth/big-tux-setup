{
    "archinstall-language": "Italian",
    "audio": "pipewire",
    "bootloader": "grub-install",
    "config_version": "2.5.1",
    "debug": false,
    "hostname": "ice-braker",
    "keyboard-layout": "it",
    "mirror-region": {
        "Italy": {
            "http://archlinux.mirror.garr.it/archlinux/$repo/os/$arch": true,
            "https://archmirror.it/repos/$repo/os/$arch": true
        }
    },
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "offline": false,
