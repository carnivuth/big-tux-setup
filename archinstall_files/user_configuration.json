    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/minimal.py"
    },
    "script": "guided",
    "silent": false,
    "sys-encoding": "UTF-8",
    "sys-language": "it_IT",
    "timezone": "Europe/Rome",
    "version": "2.5.1",
    "custom-commands": [
        "git clone https://github.com/carnivuth/big-tux-setup.git",
        " cd big-tux-setup",
        "chmod +x install.sh",
        "./install.sh",
        "cd ..",
        "echo 'installing yay'",
        "git clone https://aur.archlinux.org/yay.git",
        "cd yay",
        "makepkg -si"
    ]
}